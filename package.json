    "jscov": ">= 0.5.4 < 0.6",
    "mocha-term-cov-reporter": ">= 0.1.3 < 0.2",
    "mocha": ">= 1.8.1 < 2",
    "mongojs": ">= 0.7.9 < 0.8",
    "express": ">= 3.0.1 < 4",
    "q": ">= 0.9.6 < 0.10",
    "request": ">= 2.12.0 < 3",
